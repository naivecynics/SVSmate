{
    "name": "svsmate",
    "displayName": "SVSmate",
    "description": "The Ultimate VS Code Extension for SUSTechers!",
    "version": "2.0.0",
    "publisher": "naivecynics",
    "repository": {
        "type": "git",
        "url": "https://github.com/naivecynics/SVSmate.git"
    },
    "homepage": "https://github.com/naivecynics/SVSmate",
    "bugs": {
        "url": "https://github.com/naivecynics/SVSmate/issues"
    },
    "engines": {
        "vscode": "^1.98.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onStartupFinished"
    ],
    "main": "./dist/extension",
    "contributes": {
        "configuration": {
            "title": "svsmate",
            "properties": {
                "svsmate.root": {
                    "type": "string",
                    "default": "~/.svsmate",
                    "description": "Enter a directory path if you want to customize the cache location."
                },
                "svsmate.apikey": {
                    "type": "string",
                    "default": "",
                    "description": "API key to empower the extension (deepseek api available only)."
                },
                "svsmate.collaboration.username": {
                    "type": "string",
                    "default": "",
                    "description": "Your username for collaboration features. If empty, system hostname will be used."
                }
            }
        },
        "commands": [
            {
                "command": "svsmate.BB-updateAll",
                "title": "SVSmate: Update All Materials",
                "icon": "$(sync)"
            },
            {
                "command": "svsmate.BB-updateTerm",
                "title": "SVSmate: Update One Term",
                "icon": "$(refresh)"
            },
            {
                "command": "svsmate.BB-updateCourse",
                "title": "SVSmate: Update One Course",
                "icon": "$(refresh)"
            },
            {
                "command": "svsmate.BB-downloadToWorkspace",
                "title": "SVSmate: Download File to Workspace",
                "icon": "$(cloud-download)"
            },
            {
                "command": "svsmate.BB-downloadToAiSpace",
                "title": "SVSmate: Download File to AI Suggested Directory",
                "icon": "$(debug-step-into)"
            },
            {
                "command": "svsmate.TODO-addItem",
                "title": "SVSmate: Add Task",
                "icon": "$(add)"
            },
            {
                "command": "svsmate.TODO-editTask",
                "title": "SVSmate: Edit Task",
                "icon": "$(edit)"
            },
            {
                "command": "svsmate.TODO-deleteTask",
                "title": "SVSmate: Delete Task",
                "icon": "$(trash)"
            },
            {
                "command": "svsmate.TODO-toggleTaskCheckbox",
                "title": "SVSmate: Toggle Task Checkbox",
                "icon": "$(check)"
            },
            {
                "command": "svsmate.TODO-sortByEndTime",
                "title": "SVSmate: Sort by DDL",
                "icon": "$(sort-precedence)"
            },
            {
                "command": "svsmate.TODO-sortByKinds",
                "title": "SVSmate: Sort by Kinds",
                "icon": "$(group-by-ref-type)"
            },
            {
                "command": "svsmate.TODO-searchTasks",
                "title": "SVSmate: Search Tasks",
                "icon": "$(search)"
            },
            {
                "command": "svsmate.TODO-clearSearch",
                "title": "SVSmate: Clear Search",
                "icon": "$(clear-all)"
            },
            {
                "command": "svsmate.TODO-loadICSFile",
                "title": "SVSmate: Load from .ics File",
                "icon": "$(file-submodule)"
            },
            {
                "command": "svsmate.TODO-addSubTask",
                "title": "SVSmate: Add Subtask",
                "icon": "$(add)"
            },
            {
                "command": "svsmate.TODO-generateAISubtasks",
                "title": "SVSmate: Generate AI Subtasks",
                "icon": "$(sparkle)"
            },
            {
                "command": "svsmate.PDF-generateFromPDF",
                "title": "SVSmate: Generate Code from PDF",
                "icon": "$(note)"
            },
            {
                "command": "svsmate.COLLAB-startServer",
                "title": "SVSmate: Start Collaboration Server",
                "icon": "$(server)"
            },
            {
                "command": "svsmate.COLLAB-stopServer",
                "title": "SVSmate: Stop Collaboration Server",
                "icon": "$(debug-stop)"
            },
            {
                "command": "svsmate.COLLAB-connectToServer",
                "title": "SVSmate: Connect to Server",
                "icon": "$(plug)"
            },
            {
                "command": "svsmate.COLLAB-disconnectFromServer",
                "title": "SVSmate: Disconnect from Server",
                "icon": "$(debug-disconnect)"
            },
            {
                "command": "svsmate.COLLAB-shareCurrentFile",
                "title": "SVSmate: Share Current File",
                "icon": "$(share)"
            },
            {
                "command": "svsmate.COLLAB-shareFile",
                "title": "SVSmate: Share File",
                "icon": "$(share)"
            },
            {
                "command": "svsmate.COLLAB-openSharedFile",
                "title": "SVSmate: Open Shared File",
                "icon": "$(file)"
            },
            {
                "command": "svsmate.COLLAB-showServerInfo",
                "title": "SVSmate: Show Server Info",
                "icon": "$(info)"
            },
            {
                "command": "svsmate.COLLAB-showClientInfo",
                "title": "SVSmate: Show Client Info",
                "icon": "$(info)"
            },
            {
                "command": "svsmate.COLLAB-discoverServers",
                "title": "SVSmate: Discover Servers",
                "icon": "$(search)"
            },
            {
                "command": "svsmate.COLLAB-connectToDiscoveredServer",
                "title": "SVSmate: Connect to Discovered Server",
                "icon": "$(plug)"
            },
            {
                "command": "svsmate.COLLAB-refreshDiscoveredServers",
                "title": "SVSmate: Refresh Discovered Servers",
                "icon": "$(refresh)"
            },
            {
                "command": "svsmate.COLLAB-sendMessage",
                "title": "SVSmate: Send Message",
                "icon": "$(comment)"
            },
            {
                "command": "svsmate.COLLAB-showLatestMessage",
                "title": "SVSmate: Show Latest Message",
                "icon": "$(comment)"
            },
            {
                "command": "svsmate.COLLAB-changeUsername",
                "title": "SVSmate: Change Username",
                "icon": "$(account)"
            }
        ],
        "chatParticipants": [
            {
                "id": "svsmate.ChatBot",
                "fullName": "SVSmate",
                "name": "mate",
                "description": "Hi SUSTecher, what can I assist you?",
                "isSticky": true
            }
        ],
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "SVSmate",
                    "title": "SVSmate",
                    "icon": "media/telescope.png"
                }
            ]
        },
        "views": {
            "SVSmate": [
                {
                    "id": "folderView",
                    "name": "File Explorer"
                },
                {
                    "id": "bbMaterialView",
                    "name": "BB Materials"
                },
                {
                    "id": "todoListView",
                    "name": "To-Do List"
                },
                {
                    "id": "sharedFilesView",
                    "name": "shared files"
                }
            ]
        },
        "menus": {
            "view/item/context": [
                {
                    "command": "svsmate.TODO-editTask",
                    "when": "view == todoListView",
                    "group": "inline"
                },
                {
                    "command": "svsmate.TODO-deleteTask",
                    "when": "view == todoListView",
                    "group": "inline"
                },
                {
                    "command": "svsmate.TODO-addSubTask",
                    "when": "view == todoListView && viewItem == task",
                    "group": "inline"
                },
                {
                    "command": "svsmate.TODO-generateAISubtasks",
                    "when": "view == todoListView && viewItem == task",
                    "group": "inline"
                },
                {
                    "command": "svsmate.BB-downloadToAiSpace",
                    "when": "view == bbMaterialView && viewItem == file",
                    "group": "inline"
                },
                {
                    "command": "svsmate.BB-downloadToWorkspace",
                    "when": "view == bbMaterialView && viewItem == file",
                    "group": "inline"
                },
                {
                    "command": "svsmate.BB-updateTerm",
                    "when": "view == bbMaterialView && viewItem == termFolder",
                    "group": "inline"
                },
                {
                    "command": "svsmate.BB-updateCourse",
                    "when": "view == bbMaterialView && viewItem == courseFolder",
                    "group": "inline"
                },
                {
                    "command": "svsmate.COLLAB-shareFile",
                    "when": "view == folderView && viewItem == file",
                    "group": "inline"
                },
                {
                    "command": "svsmate.COLLAB-showServerInfo",
                    "when": "view == sharedFilesView && viewItem == serverStatus",
                    "group": "inline"
                },
                {
                    "command": "svsmate.COLLAB-showClientInfo",
                    "when": "view == sharedFilesView && viewItem == clientStatus",
                    "group": "inline"
                },
                {
                    "command": "svsmate.COLLAB-connectToDiscoveredServer",
                    "when": "view == sharedFilesView && viewItem == discoveredServer",
                    "group": "inline"
                },
                {
                    "command": "svsmate.COLLAB-refreshDiscoveredServers",
                    "when": "view == sharedFilesView && viewItem == discoveredServers",
                    "group": "inline"
                },
                {
                    "command": "svsmate.COLLAB-showLatestMessage",
                    "when": "view == sharedFilesView && viewItem == messageStatus",
                    "group": "inline"
                }
            ],
            "view/title": [
                {
                    "command": "svsmate.TODO-addItem",
                    "when": "view == todoListView",
                    "group": "navigation",
                    "title": "Add Item"
                },
                {
                    "command": "svsmate.TODO-sortByEndTime",
                    "when": "view == todoListView",
                    "group": "navigation",
                    "title": "Sort By End Time"
                },
                {
                    "command": "svsmate.TODO-sortByKinds",
                    "when": "view == todoListView",
                    "group": "navigation",
                    "title": "Sort By Kinds"
                },
                {
                    "command": "svsmate.TODO-searchTasks",
                    "when": "view == todoListView",
                    "group": "navigation",
                    "title": "Search Tasks"
                },
                {
                    "command": "svsmate.TODO-clearSearch",
                    "when": "view == todoListView ",
                    "group": "navigation",
                    "title": "Clear Search"
                },
                {
                    "command": "svsmate.TODO-loadICSFile",
                    "when": "view == todoListView",
                    "group": "navigation",
                    "title": "Load From .ics File"
                },
                {
                    "command": "svsmate.BB-updateAll",
                    "when": "view == bbMaterialView",
                    "group": "navigation",
                    "title": "Update All"
                },
                {
                    "command": "svsmate.BB-updateTerm",
                    "when": "view == bbMaterialView",
                    "group": "navigation",
                    "title": "Update Term"
                },
                {
                    "command": "svsmate.PDF-generateFromPDF",
                    "when": "view == folderView",
                    "group": "navigation",
                    "title": "Generate Code from PDF"
                },
                {
                    "command": "svsmate.COLLAB-startServer",
                    "when": "view == sharedFilesView",
                    "group": "navigation",
                    "title": "Start Server"
                },
                {
                    "command": "svsmate.COLLAB-stopServer",
                    "when": "view == sharedFilesView",
                    "group": "navigation",
                    "title": "Stop Server"
                },
                {
                    "command": "svsmate.COLLAB-connectToServer",
                    "when": "view == sharedFilesView",
                    "group": "navigation",
                    "title": "Connect to Server"
                },
                {
                    "command": "svsmate.COLLAB-shareCurrentFile",
                    "when": "view == sharedFilesView",
                    "group": "navigation",
                    "title": "Share Current File"
                },
                {
                    "command": "svsmate.COLLAB-discoverServers",
                    "when": "view == sharedFilesView",
                    "group": "navigation",
                    "title": "Discover Servers"
                },
                {
                    "command": "svsmate.COLLAB-sendMessage",
                    "when": "view == sharedFilesView",
                    "group": "navigation",
                    "title": "Send Message"
                },
                {
                    "command": "svsmate.COLLAB-changeUsername",
                    "when": "view == sharedFilesView",
                    "group": "navigation",
                    "title": "Change Username"
                }
            ],
            "editor/context": [
                {
                    "command": "svsmate.COLLAB-shareCurrentFile",
                    "group": "collaboration"
                }
            ]
        }
    },
    "dependencies": {
        "cheerio": "^1.0.0",
        "fetch-cookie": "^3.1.0",
        "node-fetch": "^2.7.0",
        "node-ical": "^0.20.1",
        "openai": "^4.87.3",
        "pdf-lib": "^1.17.1",
        "pdf-parse": "^1.1.1",
        "tough-cookie": "^5.1.2",
        "xml2js": "^0.6.2",
        "y-protocols": "^1.0.6",
        "y-webrtc": "^10.3.0",
        "yjs": "^13.6.26"
    },
    "devDependencies": {
        "@types/cheerio": "^0.22.35",
        "@types/mocha": "^10.0.10",
        "@types/node": "^20.17.46",
        "@types/node-fetch": "^2.6.12",
        "@types/pdf-parse": "^1.1.5",
        "@types/sinon": "^17.0.4",
        "@types/vscode": "^1.98.0",
        "@types/xml2js": "^0.4.14",
        "@typescript-eslint/eslint-plugin": "^8.25.0",
        "@typescript-eslint/parser": "^8.25.0",
        "@vscode/test-cli": "^0.0.10",
        "@vscode/test-electron": "^2.5.2",
        "cyclomatic-complexity": "^1.2.5",
        "esbuild": "^0.25.4",
        "eslint": "^9.21.0",
        "mocha": "^11.2.2",
        "npm-run-all": "^4.1.5",
        "sinon": "^20.0.0",
        "typedoc": "^0.28.4",
        "typescript": "^5.7.3"
    },
    "scripts": {
        "lint": "eslint src --fix",
        "docs": "typedoc",
        "dev:compile": "npx rimraf out && npx tsc -p ./ && npm run lint",
        "test": "npm run dev:compile && vscode-test",
        "compile": "npm run check-types && node esbuild.js",
        "check-types": "tsc --noEmit",
        "watch": "npm-run-all -p watch:*",
        "watch:esbuild": "node esbuild.js --watch",
        "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
        "vscode:prepublish": "npm run package",
        "package": "npm run check-types && node esbuild.js --production",
        "esbuild-base": "node esbuild.js",
        "esbuild": "npm run esbuild-base -- --sourcemap",
        "esbuild-watch": "npm run esbuild-base -- --sourcemap --watch"
    }
}
