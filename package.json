{
  "name": "svsmate",
  "displayName": "SVSmate",
  "description": "SUSTech+VSCode+Mate",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.98.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "svsmate",
      "properties": {
        "svsmate.root": {
          "type": "string",
          "default": "~/",
          "description": "the root directory for svsmate where svsmate will store all files."
        },
        "svsmate.apikey": {
          "type": "string",
          "default": "sk-c0e9243de07945c28e782f57929909b0",
          "description": "API key to power the plugin."
        }
      }
    },
    "commands": [
      {
        "command": "svsmate.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "svsmate.crawlBB",
        "title": "svsmate: Crawl Blackboard PDFs"
      },
      {
        "command": "svsmate.organizeFiles",
        "title": "svsmate: Organize Files with AI"
      },
      {
        "command": "svsmate.updateCourseJson",
        "title": "svsmate: Update Course JSON"
      }
    ],
    "chatParticipants": [
      {
        "id": "svsmate.ChatBot-API",
        "fullName": "svsmate-ChatBot-API",
        "name": "mate-API",
        "description": "What can I assist you?",
        "isSticky": true
      },
      {
        "id": "svsmate.ChatBot",
        "fullName": "svsmate-ChatBot",
        "name": "mate",
        "description": "What can I assist you?",
        "isSticky": true
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "axios-cookiejar-support": "^5.0.5",
    "cheerio": "^1.0.0",
    "fetch-cookie": "^3.1.0",
    "js-yaml": "^4.1.0",
    "node-fetch": "^2.7.0",
    "openai": "^4.87.3",
    "tough-cookie": "^5.1.2",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.35",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.17.28",
    "@types/node-fetch": "^2.6.12",
    "@types/vscode": "^1.98.0",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.21.0",
    "typescript": "^5.7.3"
  }
}
